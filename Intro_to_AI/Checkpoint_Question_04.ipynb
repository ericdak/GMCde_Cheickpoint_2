    "def remove_char(str, n):\n",
    "      first_part = str[:n] \n",
    "      last_part = str[n+1:]\n",
    "      return first_part + last_part\n",
    "print(remove_char('Kitten', 1))\n",
    "print(remove_char('Kitten', 0))\n",
    "print(remove_char('Kitten', 4))"
   ]
